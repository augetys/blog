<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hope.blog.log.mapper.SysLogMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.hope.blog.log.model.SysLog">
        <id column="id" property="id"/>
        <result column="user_name" property="userName"/>
        <result column="user_id" property="userId"/>
        <result column="ip" property="ip"/>
        <result column="ip_address" property="ipAddress"/>
        <result column="url" property="url"/>
        <result column="type" property="type"/>
        <result column="class_path" property="classPath"/>
        <result column="method" property="method"/>
        <result column="params" property="params"/>
        <result column="operation" property="operation"/>
        <result column="create_time" property="createTime"/>
        <result column="spend_time" property="spendTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        user_name,
        user_id,
        ip,
        ip_address,
        url,
        type,
        class_path
        method,
        params,
        operation,
        create_time,
        spend_time
    </sql>
</mapper>
